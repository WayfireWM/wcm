<?xml version="1.0"?>
<wayfire>
	<plugin name="oswitch">
	<_short>Output Switch</_short>
	<category>Desktop</category>
	<option name="next_output" type="activator">
		<_short>Next Output</_short>
		<default>&lt;super&gt; KEY_K</default>
	</option>
	<option name="next_output_with_win" type="activator">
		<_short>Next Output with Window</_short>
		<default>&lt;super&gt; &lt;shift&gt; KEY_K</default>
	</option>
	</plugin>
</wayfire>
